/** 

* es un traducutor

*/





public class ejercicio03 {
 public static void main (String[] args) {


 String rojo = "\033[31m";
  String verde = "\033[32m";
  String naranja = "\033[33m";
  String azul = "\033[34m";
  String morado = "\033[35m";
  String blanco = "\033[37m";
  
  System.out.println(verde + "windows\t" + verde + "ventana\t");
  System.out.println(naranja + "look\t" + naranja + "ver\t");
  System.out.println(rojo + "table\t" + rojo + "mesa\t");
  System.out.println(azul + "can\t" + azul + "poder\t");
  System.out.println(morado + "do\t" + morado + "hacer\t");
  System.out.println(blanco + "eyes\t" + blanco + "ojos\t");
  System.out.println(rojo + "one\t" + rojo + "uno\t");
  System.out.println(azul + "tv\t" + azul + "television\t");
  System.out.println(rojo + "mouse\t" + rojo + "raton\t");
  System.out.println(verde + "computer\t" + verde + "computador\t");
 

 }
 
}
